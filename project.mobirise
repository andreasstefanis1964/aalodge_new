          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features03 authorm5\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <header>Cards</header>\n        <input type=\"range\" inline title=\"Amount\" name=\"card\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"checkbox\" title=\"Card Icon\" name=\"showIcon\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#f2ac2b\" name=\"icon\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Icon BG\" value=\"#171827\" name=\"iconBg\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Icon Border\" value=\"#373d52\" name=\"border\" condition=\"showIcon\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Card Arrow\" name=\"showArrow\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#1e6bff\" name=\"arrow\" condition=\"showIcon\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1-2.jpg\" parallax selected>\n            <input type=\"color\" title=\"Color\" value=\"#232b45\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1-2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232b45\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"title-wrapper\" mbr-if=\"showTitle\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\">Some of the Facilities</h2>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"items-wrapper\">\n                    <div class=\"card-wrap\">\n                        <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont icon54-v3-smart-tv-hd\"></span>\n                        </div>\n                        <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text, .icon-wrapper\" mbr-if=\"showCardText\" mbr-theme-style=\"display-7\">Streaming service (like Netflix)</p>\n                    </div>\n                    <div class=\"card-wrap\" mbr-if=\"card > 1\">\n                        <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont icon54-v1-air-conditioner1\"></span>\n                        </div>\n                        <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text, .icon-wrapper\" mbr-if=\"showCardText\" mbr-theme-style=\"display-7\">Air conditioning / Heating</p>\n                        <div class=\"arrow-wrap\" mbr-if=\"showArrow\">\n                            <span class=\"mbr-iconfont mobi-mbri-right mobi-mbri\"></span>\n                        </div>\n                    </div>\n                    <div class=\"card-wrap\" mbr-if=\"card > 2\">\n                        <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont imind-coffee\"></span>\n                        </div>\n                        <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text, .icon-wrapper\" mbr-if=\"showCardText\" mbr-theme-style=\"display-7\">Tea/Coffee maker</p>\n                        <div class=\"arrow-wrap\" mbr-if=\"showArrow\">\n                            <span class=\"mbr-iconfont mobi-mbri-right mobi-mbri\"></span>\n                        </div>\n                    </div>\n                    <div class=\"card-wrap\" mbr-if=\"card > 3\">\n                        <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                            <span mbr-icon class=\"mbr-iconfont mbri-credit-card\"></span>\n                        </div>\n                        <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text, .icon-wrapper\" mbr-if=\"showCardText\" mbr-theme-style=\"display-7\">We accept credit/debit cards</p>\n                        <div class=\"arrow-wrap\" mbr-if=\"showArrow\">\n                            <span class=\"mbr-iconfont mobi-mbri-right mobi-mbri\"></span>\n                   